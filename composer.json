{
    "name": "rakibahmed1212/api-crud-package",
    "description": "Laravel package to generate a complete API CRUD operation",
    "keywords": [
        "rakibahmed",
        "rakibahmed1212",
        "api-crud-package"
    ],
    "homepage": "https://github.com/rakibahmed1212/api-crud-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rakib Ahmed",
            "email": "rakibahmed207@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10"
    },
    "require-dev": {
        "orchestra/testbench": "^8",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rakibahmed\\ApiCrudPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rakibahmed\\ApiCrudPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rakibahmed\\ApiCrudPackage\\ApiCrudPackageServiceProvider"
            ],
            "aliases": {
                "ApiCrudPackage": "Rakibahmed\\ApiCrudPackage\\ApiCrudPackageServiceProvider"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}